  <node pkg="autoware_vehicle_interface_prototype" name="serial_oau_node" exec="serial_oau_node"/>
  <node pkg="autoware_vehicle_interface_prototype" name="vehicle_interface_node" exec="vehicle_interface_node">
    <param from="$(var vehicle_interface_param_file)"/>
  </node>

</launch>
